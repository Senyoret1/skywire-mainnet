{
  "/api": {
    "target": "https://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": true
  },
  "/http-api": {
    "target": "http://127.0.0.1:8000",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/http-api" : "/api"
    }
  },
  "/discovery-api": {
    "target": "http://127.0.0.1:8001",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/discovery-api" : "/api"
    }
  }
}
